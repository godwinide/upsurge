      function copyToClipboard(element) {
        var $temp = $("<input>");
        $("body").append($temp);
        $temp.val($(element).text()).select();
        document.execCommand("copy");
        $temp.remove();

        /* Alert the copied text */
        Swal.fire({
          icon: "success",
          html:
            "<b>" +
            $(element).text() +
            "</b><br><br> <b style='color: red;'>Copied!</b>",
          showConfirmButton: true,
          timer: 2500,
        });
      }
    </script>